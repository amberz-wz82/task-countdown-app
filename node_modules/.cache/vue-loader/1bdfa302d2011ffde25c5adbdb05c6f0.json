InputCountdown.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <div class='announcementInput'>\n    <!-- <h4>No countdown yet. Enter one below to display.</h4> -->\n    <div id=\"input-box\">\n      <h3>Add a New Task:</h3>\n      <input type=\"text\" v-model=\"task\" placeholder=\"New task name...\" class=\"input-margin\">\n\n      <h3>Add a Deadline:</h3>\n      <input type=\"date\" v-model=\"deadline\" class=\"input-margin\">\n\n      <h3>Add Collaborators:</h3>\n      <input type=\"text\" v-model=\"collaborator\" placeholder=\"Separate by comma...\">\n      <p class=\"input-margin\">Collaborators: {{ collaborator }}</p>\n\n      <button v-on:click=\"send()\">Start Countdown</button>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'InputCountdown',\n  data: function () {\n    return {\n      // Data properties go here.\n      task: \"\",\n      deadline: \"\",\n      collaborator: \"\"\n    }\n  },\n  methods: {\n    send () {\n      // $emit code goes here\n      // console.log(\"This is where your $emit code should go!\")\n      this.$emit('taskEntered', this.task)\n      this.$emit('deadlineEntered', this.deadline)\n      this.$emit('collaboratorEntered', this.collaborator)\n    }\n  }\n}\n</script>\n\n<style>\n\nh3 {\n  margin-bottom: 5px;\n}\n\ninput {\n  border: 1px solid black;\n  border-radius: 3px;\n  padding: 10px;\n  font-size: 20px;\n}\n\n.input-margin {\n  margin-bottom: 30px;\n}\n\nbutton {\n  border: 1px solid black;\n  border-radius: 3px;\n  padding: 10px;\n  margin-bottom: 30px;\n  font-size: 20px;\n}\n\n#input-box {\n  margin-bottom: 10px;\n}\n/* \n#input-box * {\n  display: block;\n  margin-left: auto;\n  margin-right: auto;\n} */\n\n/* Sidebar */\n/* .announcementInput {\n  display: flex;\n  flex-grow: 1;\n  flex-wrap: wrap;\n  width: 20%;\n  border-right: 5px solid #769396;\n  background-color: #b3d5d8;\n  padding-left: 20px;\n} */\n</style>\n"]}]}